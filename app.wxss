/* 自定义按钮 */
.mybtn{
  position: relative;
}
.cobtn {
	width: 126rpx;
	height: 52rpx;
	border: 1px solid #3994ff;
	border-radius: 52rpx;
	color: #3994ff;
	display: flex;
	align-items: center;
	justify-content: center;
	margin: 14rpx 0;
}
.mybtn::before{
  position: absolute;
  top: 50%;
  left: 50%;
  width: 100%;
  height: 100%;
  background-color: #000;
  border: inherit;
  border-color: #000;
  border-radius: inherit;
  -webkit-transform: translate(-50%,-50%);
  transform: translate(-50%,-50%);
  opacity: 0;
  content: ' ';
}
.mybtn:active::before{
  opacity: .1;
}
/* 入驻广告 */
.advert{
  width: 120rpx;
  height: 120rpx;
  position: fixed;
  right: 0;
  bottom: 25%;
  z-index: 99;
}

.TE{
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.error{
  color: #F56C6C;
}
.success{
  color: #67c23a;

}
.warning{
  color: #E6A23C;

}