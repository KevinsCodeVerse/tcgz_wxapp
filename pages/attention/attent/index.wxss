.mynotice_box {
  display: flex;
  width: 664rpx;
height: 62rpx;
background: #f2f8ff;
border-radius: 32rpx;
  align-items: center;  
}

.mynotice_box .icon {
  width: 36rpx;
  height: 36rpx;
  margin: 0 20rpx;
}



.mynotice_box .text {
  color: #3994FF;
  width: 80%;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}

