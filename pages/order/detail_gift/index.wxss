.detail_order{
  padding: 20rpx 26rpx 100rpx 26rpx;
}
.active1{
  color: #FF6040;
}
.active2{
  color: #FF7800;
}
.active3{
  color: #3994FF;
}
.active4{
  color: #999999;
}
/* 订单进度 */
.progress{
  background-color: #FFF2F2;
  color: #FF3333;
  text-align: center;
  font-size: 24rpx;
  border-radius: 20rpx;
  padding: 24rpx 0;
  margin-bottom: 20rpx;
}
.progress2{
  background-color: #FFF2E6;
  color: #FF7800;
}
.progress3{
  background-color: #F2F8FF;
  color: #3994FF;
}
.progress .mydown{
  display: flex;
  justify-content: center;
  align-items: center;
}
.progress .van-count-down{
  color: #ff3333;
  font-size: 24rpx;
}
/* 列表css */
.orcard{
  background-color: #fff;
  border-radius: 20rpx;
  margin-bottom: 26rpx;
}
.orcard .shop{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 80rpx;
  padding: 0 24rpx;
  border-bottom: 1px solid #f1f1f1;
  font-size: 24rpx;
}
.orcard .shop>.house{
  font-weight: bold;
  display: flex;
  align-items: center;
  width: 410rpx;
}
.orcard .shname{
  width: calc(100% - 50rpx);
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.orcard .shopimg{
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}
.orcard .goshop{
  color: #999999;
  font-size: 20rpx;
  display: flex;
  align-items: center;
}
.orcard .cover{
  width: 120rpx;
  height: 120rpx;
  border-radius: 12rpx;
}
.orcard .goods{
  display: flex;
  align-items: center;
  padding: 22rpx 24rpx;
}
.orcard .goinfo{
  padding-left: 20rpx;
  width: calc(100% - 120rpx);
  box-sizing: border-box;
}
.orcard .gotit{
  font-weight: bold;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.orcard .gospec{
  font-size: 20rpx;
  color: #AAAAAA;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.orcard .count{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 10rpx;
}
.orcard .goprice{
  color: #FF7800;
  font-size: 32rpx;
  font-weight: bold;
}
.orcard .original{
  font-size: 20rpx;
  color: #AAAAAA;
  text-decoration: line-through;
}
.orcard .bag{
  font-size: 20rpx;
  color: #fff;
  background-color: #FF6040;
  border-radius: 6rpx;
  padding: 2rpx 10rpx;
  margin-left: 10rpx;
  position: relative;
  bottom: 4rpx;
}
.orcard .gonum{
  font-size: 24rpx;
  color: #aaa;
}
.orcard .handle{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 24rpx 24rpx;
  border-top: 1px solid #f1f1f1;
  font-size: 24rpx;
}
.orcard .total{
  font-weight: bold;
}
.orcard .money{
  font-size: 32rpx;
  color: #FF7800;
  margin-left: 16rpx;
}
.orcard .orbtn{
  display: flex;
}
.orcard .btn1{
  width: 144rpx;
  height: 52rpx;
  line-height: 52rpx;
  border-radius: 52rpx;
  text-align: center;
  border: 1px solid #666666;
  font-size: 22rpx;
  margin-left: 20rpx;
  font-weight: bold;
}
.orcard .btn2{
  color: #FF6040;
  border-color: #FF6040;
}
.orcard .btn3{
  color: #3994FF;
  border-color: #3994FF;
}

/* 联系 */
.contact{
  border-radius: 20rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}
.cotit{
  font-size: 24rpx;
  font-weight: bold;
  padding: 22rpx 24rpx;
  border-bottom: 1px solid #f1f1f1;
}
.contact .d_contact{
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  color: #999;
  padding: 16rpx 24rpx;
}
.contact .phone{
  width: calc(100% - 160rpx);
}
.contact .address{
  display: flex;
  margin-bottom: 6rpx;
}
.contact .address>text:first-child{
  width: 60rpx;
}
.contact .address .addre{
  width: calc(100% - 60rpx);
}
.contact .cobtn{
  width: 126rpx;
  height: 52rpx;
  border: 1px solid #3994FF;
  border-radius: 52rpx;
  color: #3994FF;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 14rpx 0;
}
.contact .cobtn image{
  width: 24rpx;
  height: 24rpx;
  margin-right: 10rpx;
}
.contact .cobtn2{
  border-color: #FF6040;
  color: #FF6040;
}
/* 订单信息 */
.basics{
  border-radius: 20rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}
.basics .balist{
  padding: 16rpx 24rpx;
}
.basics .bacard{
  display: flex;
  justify-content: space-between;
  color: #999999;
  font-size: 24rpx;
  height: 44rpx;
  line-height: 44rpx;
}
.basics .label{
  color: #3994FF;
  border: 1px solid #3994FF;
  width: 96rpx;
  height: 44rpx;
  line-height: 44rpx;
  text-align: center;
  border-radius: 44rpx;
}
/* 底部栏 */
.foot{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 36rpx;
  background-color: #3994FF;
}
/* 地址信息 */
.delivery{
  padding: 20rpx;
  color: #999;
  font-size: 24rpx;
}
.delivery view:first-child{
  margin-bottom: 6rpx;
}
.delivery .name{
  font-size: 28rpx;
  font-weight: bold;
  color: #000;
  margin-right: 20rpx;
}
/* 商品评价 */
.evaluate{
  background-color: #fff;
  margin-bottom: 18rpx;
  border-radius: 20rpx;
}
.evaluate .evcard{
  display: flex;
}
.evaluate .avatar{
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.evaluate .name{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-size: 24rpx;
  width: calc(100% - 92rpx);
}
.evaluate .time{
  display: flex;
  justify-content: space-between;
}
.evaluate .time>text:last-child{
  color: #B6B6B6;
}
.evaluate .evcon{
  margin-top: 20rpx;
}
.evaluate .evimgs{
  display: flex;
  align-items: center;
  margin-top: 20rpx;
}
.evaluate .evimgs image{
  width: 210rpx;
  height: 210rpx;
  border-radius: 16rpx;
  margin-right: 20rpx;
}
.evaluate .evimgs image:last-child{
  margin-right: 0;
}
/* -------------------------------------------------秒杀------------------------------------------------ */

