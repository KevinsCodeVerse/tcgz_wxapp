/* 提示 */
.text_box {
	background-color: #fff;
	border-radius: 20rpx;
	margin-bottom: 24rpx;
	padding: 24rpx;
	font-weight: 700;
	font-size: 24rpx;
}

/* 订单进度 */
.progress {
	background-color: #ff7800;
	color: #ffffff;
	font-size: 28rpx;
	border-radius: 20rpx;
	padding: 24rpx 24rpx;
	margin-bottom: 20rpx;
}
.progress2 {
	font-weight: 600;

	background-color: #ff6666;
	color: #fff;
}
.progress4 {
	font-weight: 600;

	background-color: #3994ff;
	color: #fff;
}
.times {
	opacity: 0.6;
	font-weight: 400;
}
.progress3 {
	font-weight: 600;
	background-color: #ffffff;
	color: rgba(0, 0, 0, 0.781);
}
.progress .mydown {
	display: flex;
	color: #ffffff;
	opacity: 0.8;
	z-index: 22 !important;
}
.progress .van-count-down {
	color: #ffffff;
	opacity: 1;
	font-size: 28rpx;
}
/* 列表css */
.orcard {
	background-color: #fff;
	border-radius: 20rpx;
	margin-bottom: 26rpx;
}
.orcard .shop {
	display: flex;
	justify-content: space-between;
	align-items: center;
	height: 80rpx;
	padding: 0 24rpx;
	border-bottom: 1px solid #f1f1f1;
	font-size: 24rpx;
}
.orcard .shop > .house {
	font-weight: bold;
	display: flex;
	align-items: center;
	width: 410rpx;
}
.orcard .shname {
	width: calc(100% - 50rpx);
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
}
.orcard .shopimg {
	width: 40rpx;
	height: 40rpx;
	border-radius: 50%;
	margin-right: 10rpx;
}
.orcard .goshop {
	color: #999999;
	font-size: 20rpx;
	display: flex;
	align-items: center;
}
.orcard .cover {
	border: 1rpx solid #66666623;

	width: 120rpx;
	height: 120rpx;
	border-radius: 12rpx;
}
.orcard .goods {
	display: flex;
	align-items: center;
	padding: 22rpx 24rpx;
	padding-bottom: 0;
}
.orcard .goinfo {
	padding-left: 20rpx;
	width: calc(100% - 120rpx);
	box-sizing: border-box;
}
.orcard .goinfo {
	padding-left: 20rpx;
	width: calc(100% - 120rpx);
	box-sizing: border-box;
}
.orcard .gotit {
	display: flex;
	justify-content: space-between;
}
.orcard .gotit > text {
	font-size: 18rpx;
	border-radius: 10rpx;
	word-break: keep-all;
	line-height: 36rpx;
	padding: 0rpx 8rpx;
	background-color: green;
}
.orcard .gotit .logistics {
	background-color: #fff2f2;
	color: #ff3333;
}
.orcard .gotit .local {
	color: #ff7800;

	background-color: #fff2e6;
}
.orcard .gotit > view {
	font-weight: bold;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
}

.orcard .gospec {
	font-size: 20rpx;
	color: #aaaaaa;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
}
.orcard .count {
	display: flex;
	justify-content: space-between;
	align-items: center;
	margin-top: 10rpx;
}
.orcard .goprice {
	color: #ff7800;
	font-size: 32rpx;
	font-weight: bold;
}
.orcard .original {
	font-size: 20rpx;
	color: #aaaaaa;
	text-decoration: line-through;
}
.orcard .freight {
	font-size: 20rpx;
	color: #aaaaaa;
	margin: 0 15rpx;
}
.orcard .bag {
	font-size: 20rpx;
	color: #fff;
	background-color: #ff6040;
	border-radius: 6rpx;
	padding: 2rpx 10rpx;
	margin-left: 10rpx;
	position: relative;
	bottom: 4rpx;
}
.orcard .gonum {
	font-size: 24rpx;
	color: #aaa;
}
.orcard .handle {
	display: flex;
	align-items: center;
	justify-content: space-between;
	padding: 24rpx 24rpx;
	border-top: 1px solid #f1f1f1;
	font-size: 24rpx;
}
.orcard .total {
	font-weight: bold;
}
.orcard .money {
	font-size: 32rpx;
	color: #ff7800;
	margin-left: 16rpx;
}
.orcard .orbtn {
	display: flex;
}
.orcard .btn1 {
	width: 144rpx;
	height: 52rpx;
	line-height: 52rpx;
	border-radius: 52rpx;
	text-align: center;
	border: 1px solid #666666;
	font-size: 22rpx;
	margin-left: 20rpx;
	font-weight: bold;
}
.orcard .btn2 {
	color: #ff6040;
	border-color: #ff6040;
}
.orcard .btn3 {
	color: #3994ff;
	border-color: #3994ff;
}

/* 联系 */
.contact {
	position: relative;
	border-radius: 20rpx;
	background-color: #fff;
	margin-bottom: 20rpx;
}
.cotit {
	font-size: 24rpx;
	font-weight: bold;
	padding: 22rpx 24rpx;
	border-bottom: 1px solid #f1f1f1;
}
.contact .d_contact {
	display: flex;
	justify-content: space-between;
	align-items: center;
	font-size: 24rpx;
	color: #999;
	padding: 16rpx 24rpx;
}
.contact .phone {
	width: calc(100% - 160rpx);
}
.contact .address {
	display: flex;
	margin-bottom: 6rpx;
}
.contact .bor {
	padding-bottom: 15rpx;
	margin-bottom: 5rpx;
	border-bottom: 1px solid #f1f1f1;
}

.contact .address .addre {
	width: calc(100% - 110rpx);
	word-wrap: break-word;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
}
.contact .cobtn {
	width: 126rpx;
	height: 52rpx;
	border: 1px solid #3994ff;
	border-radius: 52rpx;
	color: #3994ff;
	display: flex;
	align-items: center;
	justify-content: center;
	margin: 14rpx 0;
}
.btt {
	margin-top: 40rpx !important;
}
.contact .cobtn image {
	width: 24rpx;
	height: 24rpx;
	margin-right: 10rpx;
}
.contact .cobtn2 {
	border-color: #ff6040;
	color: #ff6040;
}
/* 订单信息 */
.basics {
	border-radius: 20rpx;
	background-color: #fff;
	margin-bottom: 20rpx;
}
.basics .balist {
	padding: 16rpx 24rpx;
}
.basics .bacard {
	display: flex;
	justify-content: space-between;
	color: #999999;
	font-size: 24rpx;
	line-height: 44rpx;
	padding: 0rpx 10rpx;
}

.basics .label {
	color: #3994ff;
	border: 1px solid #3994ff;
	width: 96rpx;
	height: 44rpx;
	line-height: 44rpx;
	text-align: center;
	border-radius: 44rpx;
}
/* 底部栏 */
.foot {
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	height: 100rpx;
	line-height: 100rpx;
	text-align: center;
	color: #fff;
	font-size: 36rpx;
	background-color: #3994ff;
}
/* 核销二维码 */
.ewm {
	border-radius: 20rpx;
	background-color: #fff;
	margin-bottom: 20rpx;
}
.ewm .ewmcon {
	text-align: center;
	font-weight: bold;
	padding: 40rpx 0;
}
.ewm .ewmimg {
	width: 310rpx;
	margin-top: 6rpx;
	display: inline-block;
	position: relative;
}
.ewm .ewmimg .ewmimage {
	width: 100%;
	height: 310rpx;
}
.ewm .ewmimg .shade {
	position: absolute;
	z-index: 4;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	background-color: rgba(255, 255, 255, 0.9);
}
.ewm .finish {
	width: 160rpx;
	height: 160rpx;
	margin-top: 75rpx;
}
/* 地址信息 */
.delivery {
	padding: 20rpx;
	color: #999;
	font-size: 24rpx;
}
.delivery view:first-child {
	margin-bottom: 6rpx;
}
.delivery .name {
	font-size: 28rpx;
	font-weight: bold;
	color: #000;
	margin-right: 20rpx;
}
/* 商品评价 */
.evaluate {
	background-color: #fff;
	margin-bottom: 18rpx;
	border-radius: 20rpx;
}
.evaluate .evcard {
	display: flex;
}
.evaluate .avatar {
	width: 72rpx;
	height: 72rpx;
	border-radius: 50%;
	margin-right: 20rpx;
}
.evaluate .name {
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	font-size: 24rpx;
	width: calc(100% - 92rpx);
}
.evaluate .time {
	display: flex;
	justify-content: space-between;
}
.evaluate .time > text:last-child {
	color: #b6b6b6;
}
.evaluate .evcon {
	margin-top: 20rpx;
}
.evaluate .evimgs {
	display: flex;
	align-items: center;
	margin-top: 20rpx;
}
.evaluate .evimgs image {
	width: 210rpx;
	height: 210rpx;
	border-radius: 16rpx;
	margin-right: 20rpx;
}
.evaluate .evimgs image:last-child {
	margin-right: 0;
}
/* -------------------------------------------------秒杀------------------------------------------------ */

.pay_box {
	display: flex;
}
.pay_box view {
	flex: 1;
}
.pay_box > view:nth-child(1) {
	background: #fff;
	color: #3994ff;
}

.seckill {
	background-color: #ff6040;
}
.bgwhie2 {
	color: #ff6040 !important;
}

.image_list_box image {
	width: 150rpx;
	height: 150rpx;
	margin: 10rpx;
	border: 1px solid #66666622;
	border-radius: 15rpx;
}
