/* pages/goods/detail/detail.wxss */
.detail{
  padding-bottom: 100rpx;
}
.ss{
	position: absolute;
	top: 45%;
	width: 100%;
	left: 50%;
	line-height: 60rpx;
	transform: translate(-50%,0);
	text-align: center;
}
/* 返回胶囊 */
.myback{
  position: absolute;
  top: 0;
  left: 20rpx;
  background-color: rgba(255, 255, 255, .6);
  font-size: 40rpx;
  border-radius: 10rpx;
  border-radius: 88rpx;
  padding: 12rpx 0;
  display: flex;
}
/* 返回胶囊 */
.myback{
  position: absolute;
  top: 6rpx;
  left: 20rpx;
  background-color: rgba(255, 255, 255, .6);
  font-size: 40rpx;
  border-radius: 10rpx;
  border-radius: 88rpx;
  padding: 12rpx 0;
  display: flex;
}
.myback>.back{
  display: flex;
  padding: 0 20rpx;
}
.myback>.back:first-child{
  border-right: 1px solid #000;
}
/* 轮播图css */
.myswiper{
  position: relative;
}
.myswiper .browse{
  background-color: rgba(0, 0, 0, .7);
  position: absolute;
  bottom: 100rpx;
  right: 0;
  color: #fff;
  font-size: 20rpx;
  padding: 10rpx 20rpx;
  border-radius: 40rpx 0 0 40rpx;
}
.bannimg{
  width: 100vw;
  height: 100vw;
}
/* 内容 */
.mycon{
  background-color: #fff;
  padding: 36rpx 30rpx 40rpx 30rpx;
  margin-bottom: 18rpx;
}
.mycon .cotit{
  word-wrap: break-word;
  padding: 0 10rpx;
  font-size: 36rpx;
  font-weight: bold;
}
.mycon .price{
  padding: 0 10rpx;
  display: flex;
  align-items: flex-end;
  justify-content: space-between;
  font-size: 24rpx;
  color: #AAAAAA;
  margin-top: 16rpx;
}
.mycon .d_price{
  display: flex;
  align-items: flex-end;
}
.mycon .curre{
  color: #FF7800;
  font-size: 44rpx;
  font-weight: bold;
  margin-right: 10rpx;
  position: relative;
  top: 6rpx;
}
.mycon .original{
  margin:0 10rpx;
  text-decoration: line-through;
}
.mycon .old{
  background: linear-gradient(180deg, #FF843E 0%, #FF4848 100%);
  color: #fff;
  padding: 0 16rpx 10rpx 16rpx;
  border-radius: 10rpx;
  margin-left: 10rpx;
  display: flex;
  align-items: flex-end;
}
.mycon .old view{
  font-size: 44rpx;
  font-weight: bold;
  position: relative;
  top: 8rpx;
}
/* 联系 */
.contact{
  box-shadow: 0px 0px 14rpx rgba(0, 0, 0, 0.14);
  margin-top: 26rpx;
  padding: 14rpx 30rpx;
  border-radius: 10rpx;
}
.contact .d_contact{
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  color: #999;
}
.contact .phone{
  width: calc(100% - 160rpx);
}
.contact .address{
  display: flex;
}
.contact .address>text:first-child{
  width: 60rpx;
}
.contact .address .addre{
  width: calc(100% - 60rpx);
}
.contact .cobtn{
  width: 126rpx;
  height: 52rpx;
  border: 1px solid #3994FF;
  border-radius: 52rpx;
  color: #3994FF;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 14rpx 0;
}
.contact .cobtn image{
  width: 24rpx;
  height: 24rpx;
  margin-right: 10rpx;
}
.contact .cobtn2{
  border-color: #FF6040;
  color: #FF6040;
}
.contact .house{
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #eee;
  padding-bottom: 20rpx;
  padding-top: 6rpx;
  margin-bottom: 10rpx;
}
.contact .housename{
  width: calc(100% - 140rpx);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.contact .holink{
  display: flex;
  align-items: center;
  font-size: 20rpx;
  color: #999;
}
/* 特殊标题 */
.tit{
  text-align: center;
  font-size: 32rpx;
  font-weight: bold;
  padding-bottom: 30rpx;
  padding-top: 10rpx;
}
.tit text{
  position: relative;
  z-index: 2;
}
.tit text::before{
  content: '';
  position: absolute;
  border-bottom: 8rpx solid #3994FF;
  bottom: 2rpx;
  left: 0;
  right: 0;
  z-index: -1;
}
.tit2 text::before{
  border-color: #FF6040;
}
/* 商品评价 */
.evaluate{
  background-color: #fff;
  padding: 34rpx 40rpx;
  margin-bottom: 18rpx;
}
.evaluate .evcard{
  display: flex;
}
.evaluate .avatar{
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.evaluate .name{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-size: 24rpx;
  width: calc(100% - 92rpx);
}
.evaluate .time{
  display: flex;
  justify-content: space-between;
}
.evaluate .time>text:last-child{
  color: #B6B6B6;
}
.evaluate .evcon{
  margin-top: 20rpx;
}
.evaluate .evimgs{
  display: flex;
  align-items: center;
  margin-top: 20rpx;
}
.evaluate .evimgs image{
  width: 210rpx;
  height: 210rpx;
  border-radius: 16rpx;
  margin-right: 20rpx;
}
.evaluate .evimgs image:last-child{
  margin-right: 0;
}
.evaluate .more{
  color: #3994FF;
  display: flex;
  align-items: center;
  justify-content: center;
  padding-bottom: 26rpx;
  padding-top: 60rpx;
  font-size: 24rpx;
}
/* 商品详情 */
.mycont{
  background-color: #fff;
  padding: 34rpx 40rpx;
  margin-bottom: 18rpx;
}
.mycont .richtext{
  line-height: 54rpx;
}
.mycon .richtext .graphic{
  width: 100%;
}
/* 底部栏 */
.foot{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  height: 100rpx;
  background-color: #fff;
  z-index: 99;
  border-top: 1px solid #ddd;
  display: flex;
  align-items: center;
  justify-content: space-between;
  text-align: center;
}
.foot image{
  width: 32rpx;
  height: 32rpx;
}
.foot .handle{
  width: 110rpx;
  font-size: 20rpx;
  color: #666666;
  box-sizing: border-box;
}
.foot .btn{
  background-color: #3994FF;
  color: #fff;
  width: calc(100% - 220rpx);
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 36rpx;
}
/* 人气推荐 */
.recommend{
  background-color: #fff;
  padding: 34rpx 0;
}
/* 商品列表 */
.product_list {
  width: 94%;
  margin: 0 auto;
  display: flex;
  flex-wrap: wrap;
}
.product_list .product_item {
  width: 45.5vw;
  margin-bottom: 3vw;
  box-shadow: 0 0 3px 3px rgba(0,0,0,0.05);
  border-radius: 5px;
  background: #ffffff;
}
.product_list .product_item:nth-child(2n-1) {
  margin-right: 3vw;
}
.product_list .product_item .product_cover {
  width: 45.5vw;
  height: 45.5vw;
  position: relative;
}
.product_list .product_item .product_cover>image {
  width: 100%;
  height: 100%;
  border-radius: 5px 5px 0 0;
}
.product_list .product_item .product_cover .price {
  position: absolute;
  width: 100%;
  height: 60rpx;
  /* background-color: rgba(0,0,0,0.5); */
  bottom: 0;
  left: 0;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.product_list .product_item .product_cover .price_num .unit {
  height: 46rpx;
  display: block;
  position: relative;
  right: -3rpx;
}
.product_list .product_item .product_cover .price>text {
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  font-size: 24rpx;
}
.product_list .product_item .product_cover .price_num {
  display: flex;
  position: absolute;
  right: 0;
  bottom: 0;
}

.product_list .product_item .product_cover .price_num .num {
  height: 46rpx;
  line-height: 46rpx;
  background: linear-gradient(to bottom,#FF843E,#FF4848);
  color: #ffffff;
  font-size: 40rpx;
  padding: 0 12rpx 0 0;
}
.product_info {
  padding: 20rpx;
  box-sizing: border-box;
}
.product_info .product_title {
  height: 80rpx;
  font-size: 28rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.product_info .info_bottom {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  margin-top: 10rpx;
}
.product_info .info_bottom .price .new {
  font-size: 32rpx;
  color: #FF7800;
}
.product_info .info_bottom .price .old {
  font-size: 20rpx;
  color: #AAAAAA;
  text-decoration-line: line-through;
  margin-left: 6rpx;
}
.product_info .info_bottom .sales {
  font-size: 24rpx;
  color: #AAAAAA;
}
/* 秒杀 */
.seck_at{
  position: relative;
  background-color: #fff;
  height: 52rpx;
}
.seckill{
  display: flex;
  margin: 0 26rpx;
  border-radius: 10rpx;
  box-shadow: 0px 10rpx 14rpx rgba(255, 96, 64, 0.3);
  overflow: hidden;
  position: absolute;
  top: -52rpx;
  left: 0;
  right: 0;
}
.seckill .seinfo{
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: linear-gradient(90deg, #FF7800 0%, #FF4B33 100%);
  color: #fff;
  width: calc(100% - 220rpx);
  padding: 0 26rpx;
  height: 104rpx;
  font-size: 20rpx;
}
.seckill .seinfo image{
  width: 60rpx;
  height: 62rpx;
  margin-right: 14rpx;
}
.seckill .seremark{
  display: flex;
  align-items: center;
}
.seckill .setit{
  font-weight: bold;
  font-size: 28rpx;
  margin-bottom: 2rpx;
}
.seckill .stock{
  text-align: center;
}
.seckill .stprice{
  font-size: 38rpx;
  font-weight: bold;
}
.seckill .time{
  width: 220rpx;
  background-color: #fff;
  color: #FF6040;
  text-align: center;
  font-size: 20rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
}
.seckill .timeitem{
  background-color: #FF6040;
  color: #fff;
  font-size: 28rpx;
  border-radius: 6rpx;
  min-width: 36rpx;
  height: 40rpx;
  padding: 0 2rpx;
  display: inline-block;
}
.seckill .mytimedown{
  color: #FF6040;
  padding: 0 6rpx;
}

.btn_custom_class {
	width: 220rpx;
	margin-right: 30rpx;
}

/* 优惠券  */
.discount_box {
	display: flex;
	margin: 20rpx 0;
	height: 80rpx;
	align-items: center;
	justify-content: space-between;
	background-color: #FFF2F2;
	border-radius: 10rpx;
}

.discount_box .reduction {
	display: flex;
	font-size: 24rpx;
}

.discount_box .reduction text {
	background: linear-gradient(90deg, #ff843e, #ff4848);
	padding: 4rpx 15rpx;
	border-radius: 8rpx;
	margin: 0 0rpx 0 20rpx;
	color: #ffffff;
	word-break: keep-all;
}
.icon {
	align-items: center;
	display: flex;
	justify-content: center;
	min-width: 100rpx;
	width: 100rpx;
}

.icon text {
	position: relative;
	top: -3rpx;
	color: #ff3333;
	font-weight: 700;
}
/* 优惠券结束  */

.desc_bottom_box .title,
.select_bottom_box .title {
	height: 90rpx;
	display: flex;
	font-size: 32rpx;
	font-weight: bold;
	justify-content: center;
	align-items: center;
	/* border-bottom: 1px solid #99999146; */
}

.desc_bottom_box .img,
.select_bottom_box .img {
	position: absolute;
	right: 30rpx;
	top: 27rpx;
}

.desc_bottom_box .bottom_close,
.select_bottom_box .bottom_close {
	position: absolute;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 90rpx;
	display: flex;
	font-size: 32rpx;
	justify-content: center;
	align-items: center;
	border-top: 1px solid #99999123;
	color: #666666;
}

.desc_bottom_box {
	position: relative;
	background-color: #f7f7f7 !important;
}

.page_box {
	height: calc(100% - 90rpx - 130rpx);
	padding: 30rpx;
	width: calc(100% - 60rpx);
	color: #444444;
	font-size: 30rpx;
}

.desc_box1 {
	width: 100%;
}

.discountList_box {
