/* 标识 */
.label_box {
	height: 70rpx;
	display: flex;
	margin: 30rpx 0 0 0;
}
.label_box .label_item {
	margin-right: 20rpx;
	padding: 0 10rpx;
	background-color: #f5f5f5;
	height: 40rpx;
	font-size: 20rpx;
	color: #999999;
	line-height: 40rpx;
	word-break: keep-all;
}
/* 标识结束 */