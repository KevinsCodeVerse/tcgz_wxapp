.contact {
	box-shadow: 0px 0px 14rpx rgba(0, 0, 0, 0.14);
	margin-top: 26rpx;
	padding: 14rpx 30rpx;
	border-radius: 20rpx;
	margin: 26rpx;
	height: 230rpx;
	background: #ffffff;
}
.contact .house {
	display: flex;
	justify-content: space-between;
	align-items: center;
	border-bottom: 1px solid #eee;
	padding-bottom: 20rpx;
	padding-top: 6rpx;
	margin-bottom: 10rpx;
	font-size: 32rpx;
	font-weight: 700;
}
.contact .d_contact {
	display: flex;
}
.contact .d_contact > view:nth-child(1) {
	margin-right: 15rpx;
}
.defaultimage {
	width: 120rpx;
	height: 120rpx;
	border-radius: 10rpx;
	margin: 15rpx 0;
}
.contact_text .title {
	display: -webkit-box;
	overflow: hidden;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 1;
	margin-top: 8rpx;
	height: 40rpx;
	font-size: 28rpx;
	font-weight: 700;
	text-align: left;
	color: #333333;
	line-height: 40rpx;
}
.contact_text .prosonNum {
	font-size: 20rpx;
	font-weight: 400;
	text-align: left;
	color: #aaaaaa;
}
.contact_text .count {
	display: flex;
	justify-content: space-between;
	align-items: center;
	margin-top: 10rpx;
}
.contact_text .goprice {
	color: #86cc14;
	font-size: 32rpx;
	font-weight: bold;
}
.contact_text .original {
	font-size: 20rpx;
	color: #aaaaaa;
	text-decoration: line-through;
}
.contact_text .bag {
	font-size: 20rpx;
	color: #fff;
	background-color: #86cc14;
	border-radius: 6rpx;
	padding: 2rpx 10rpx;
	margin-left: 10rpx;
	position: relative;
	bottom: 4rpx;
	display: inline-block;
}

.contact_text .bag text:nth-child(1) {
	position: relative;
	top: -5rpx;
}
.contact_text .bag .bigpr {
	font-size: 32rpx;
}

/* 拼团列表 */
.grolist {
	margin: 26rpx;
	box-shadow: 0px 0px 14rpx rgba(0, 0, 0, 0.14);
	margin-top: 46rpx;
	padding: 14rpx 30rpx;
	border-radius: 10rpx;
	background-color: #fff;
}
.grolist .house {
	display: flex;
	justify-content: space-between;
	align-items: center;
	border-bottom: 1px solid #eee;
	padding-bottom: 20rpx;
	padding-top: 6rpx;
	margin-bottom: 10rpx;
}
.grolist .housename {
	width: calc(100% - 140rpx);
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	font-size: 32rpx;
	font-weight: 700;
	text-align: left;
	color: #333333;
}
.grolist .housename text {
	color: #86cc14;
	font-weight: bold;
}
.grolist .holink {
	display: flex;
	align-items: center;
	font-size: 20rpx;
	color: #999;
}
.d_grolist .grocard {
	display: flex;
	align-items: center;
	justify-content: space-between;
	font-size: 24rpx;
	padding: 14rpx 0;
}
.d_grolist image {
	width: 52rpx;
	height: 52rpx;
	border-radius: 50%;
	margin-right: 10rpx;
}
.d_grolist .attr {
	display: flex;
	align-items: center;
	font-size: 20rpx;
}
.d_grolist .gruser {
	display: flex;
	align-items: center;
}
.d_grolist .gruser text {
	width: 200rpx;
	display: -webkit-box;
	text-overflow: ellipsis;
	overflow: hidden;
	word-break: break-all;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
}
.d_grolist .grbtn {
	background-color: #86cc14;
	color: #fff;
	font-size: 24rpx;
	width: 120rpx;
	height: 52rpx;
	line-height: 52rpx;
	text-align: center;
	border-radius: 52rpx;
}
.d_grolist .sttime {
	text-align: right;
	margin-right: 30rpx;
}
.d_grolist .sttime > view:first-child {
	margin-bottom: 6rpx;
}
.d_grolist .sttime text {
	color: #86cc14;
}
.d_grolist .null_text {
	height: 180rpx;
	width: 290rpx;
	text-align: center;
	position: relative;
	left: 170rpx;
	top: 50rpx;
	font-size: 24rpx;
	font-weight: 400;
	text-align: center;
	color: #999999;
	line-height: 32rpx;
	letter-spacing: 2rpx;
}

.d_grolist .btn {
	width: 320rpx;
	height: 76rpx;
	margin-bottom: 20rpx;
	background: #86cc14;
	border-radius: 40rpx;
	font-size: 32rpx;
	font-weight: 400;
	text-align: center;
	line-height: 76rpx;
	color: #ffffff;
}

