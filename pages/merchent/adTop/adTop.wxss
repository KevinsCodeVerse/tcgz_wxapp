.rule_box {
	height: 800rpx;
	width: 550rpx;
	border-radius: 30rpx;
	overflow: hidden;
	background-color: rgb(255, 255, 255);
}

.rule_box .rule_title {
	height: 100rpx;
	line-height: 100rpx;
	text-align: center;
	font-size: 30rpx;
	font-weight: 600;
}

.rule_box .rule_text {
	font-size: 28rpx;
	height: 600rpx;
	padding: 10rpx 56rpx;
	overflow: auto;
}

.rule_box .rule_btn {
	position: relative;
	bottom: -5rpx;
	height: 85rpx;
	background-color: #3994ff;
	color: #fff;
	line-height: 85rpx;
	text-align: center;
}

/* 参与竞价弹窗
 */
.desc_bottom_box {
	position: relative;
}
.page_box {
	height: calc(100% - 90rpx - 130rpx);
	padding: 0 30rpx;
	color: #444444;
	font-size: 30rpx;
	width: calc(100% - 60rpx);
}

.desc_box1 {
	width: 100%;
}
.desc_box1 view {
	margin: 20rpx 0;
}
.desc_box1 .desc {
	color: #222;
	margin: 30rpx 0 20rpx 0;
	font-size: 36rpx;
	font-weight: bold;
}
.desc_box1 .text {
	line-height: 50rpx;
}

.desc_bottom_box .title,
.select_bottom_box .title {
	height: 90rpx;
	display: flex;
	font-size: 34rpx;
	font-weight: bold;
	justify-content: center;
	align-items: center;
	border-bottom: 1px solid #99999146;
}
.desc_bottom_box .img,
.select_bottom_box .img {
	position: absolute;
	right: 30rpx;
	top: 27rpx;
}
.desc_bottom_box .bottom_close,
.select_bottom_box .bottom_close {
	position: absolute;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 90rpx;
	display: flex;
	font-size: 40rpx;
	justify-content: center;
	align-items: center;
	border-top: 1px solid #99999123;
	color: #fff;
	background-color: #3994ff;
}
.row {
	height: 100rpx;
	border-bottom: 1px solid rgba(0, 0, 0, 0.116);
	display: flex;
	justify-content: space-between;
	line-height: 100rpx;
}
.rest {
	display: flex;
	align-items: center;
}
.myminus {
	border-radius: 50% !important;
	border: 1px solid #c7c7c7 !important;
	background-color: #fff !important;
	margin-right: 20rpx !important;
}

.van-stepper__minus--disabled,
.van-stepper__plus--disabled {
