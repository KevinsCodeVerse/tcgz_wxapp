/* pages/collective/join/join.wxss */
.fillin{
  padding-bottom: 130rpx;
  background-color: #fff;
  min-height: 100vh;
  box-sizing: border-box;
}
.banner{
  width: 100%;
  height: 360rpx;
}
.join{
  padding: 0 60rpx;
}
.myinput{
  display: flex;
  align-items: center;
  border-bottom: 1px solid #ddd;
  padding: 30rpx 0;
  font-size: 30rpx;
}
.myinput .labl{
  width: 180rpx;
}
.myinput input{
  width: calc(100% - 180rpx - 40rpx);
  color: #666;
}
.myinput .joimg{
  width: 60rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  height: 48rpx;
}
.myinput image{
  width: 34rpx;
  height: 34rpx;
  text-align: right;
  display: block;
}
.mytextarea{
  align-items: flex-start;
}
.mytextarea textarea{
  width: calc(100% - 180rpx - 40rpx);
  color: #666;
  height: 100rpx;
}
.protocol{
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #999;
  margin-top: 30rpx;
}
.protocol .radio{
  width: 26rpx;
  height: 26rpx;
  margin-right: 12rpx;
}
.protocol navigator{
  color: #3994FF;
}
/* 底部css */
.foot{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  text-align: center;
  padding: 30rpx 0;
  font-size: 36rpx;
  background-color: #3994FF;
  font-size: 36rpx;
  color: #fff;
}
/*选择器 */
.myselect{
  width: 390rpx;
  color: #666;
  min-height: 1.4rem;
  line-height: 1.4rem;
}
/* 覆盖vant */
.van-picker__cancel{
  color: #7f7f7f!important;
}
.van-picker__confirm{
  color: #07c160!important;
}