/* pages/collective/votelist/votelist.wxss */
.votelist{
  background-color: #fff;
  min-height: 100vh;
  box-sizing: border-box;
}
.name{
  font-size: 28rpx;
  text-align: center;
  background-color: #f5f5f5;
  color: #666;
  padding: 40rpx 0;
}
.name view{
  font-size: 40rpx;
  font-weight: bold;
  color: #000;
  margin-bottom: 10rpx;
  letter-spacing: 6rpx;
}
/* 表格css */
.mytable{
  padding: 30rpx 30rpx;
}
.mytable .tr{
  display: flex;
  align-items: center;
  color: #666;
  font-size: 24rpx;
  padding: 14rpx 0;
}
.mytable .td:nth-child(1){
  text-align: left;
  width: 40%;
  display: flex;
  align-items: center;
}
.mytable .td:nth-child(2){
  text-align: center;
  width: 60%;
  overflow:hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.mytable .th{
  font-weight: bold;
  color: #333;
  font-size: 28rpx;
}
.mytable image{
  width: 48rpx;
  height: 48rpx;
  object-fit: cover;
  border-radius: 50%;
  margin-right: 14rpx;
}