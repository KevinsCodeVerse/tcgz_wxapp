/* pages/collective/activity_detail2/activity_detail2.wxss */
.activity_detail{
  padding-bottom: 100rpx;
  background-color: #fff;
  min-height: 100vh;
  box-sizing: border-box;
}
.cover{
  width: 100%;
  height: 375rpx;
}
/* 信息css */
.d_status{
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #999;
  font-size: 20rpx;
  padding: 30rpx 36rpx 16rpx 36rpx;
}
.d_status .mylabel text{
  padding: 6rpx 16rpx;
  color: #fff;
  display: inline-block;
  text-align: center;
  margin-right: 16rpx;
  border-radius: 6rpx;
}
.info{
  padding: 0 36rpx 36rpx 36rpx;
  font-size: 20rpx;
  color: #999;
  border-bottom: 16rpx solid #f1f1f1;
}
.info .tit{
  font-size: 36rpx;
  font-weight: bold;
  color: #333;
}
.info .subtit{
  font-size: 24rpx;
  margin-bottom: 32rpx;
  margin-top: 14rpx;
}
.info .time{
  margin-bottom: 10rpx;
}
/* 地址css */
.address{
  font-size: 24rpx;
  color: #999;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-shadow: 0px 0px 14rpx rgba(0, 0, 0, 0.14);
  margin: 40rpx 30rpx 6rpx 30rpx;
  padding: 30rpx;
  border-radius: 10rpx;
}
.d_address .lab{
  font-size: 28rpx;
  font-weight: bold;
  color: #333;
  margin-bottom: 10rpx;
}
.address .d_address{
  width: calc(100% - 180rpx);
}
.address .cobtn{
  width: 126rpx;
  height: 52rpx;
  border: 1px solid #3994FF;
  border-radius: 52rpx;
  color: #3994FF;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 14rpx 0;
}
.address .cobtn image{
  width: 24rpx;
  height: 24rpx;
  margin-right: 10rpx;
}
/* 覆盖vant */
.van-ellipsis{
  font-size: 32rpx;
  font-weight: bold;
}
.van-tabs--line .van-tabs__wrap{
  height: 108rpx!important;
}
.van-tab {
  line-height: 108rpx!important;
}
/* 活动详情 */
.atdetail{
  font-size: 26rpx;
  padding: 30rpx;
  line-height: 52rpx;
}
.atdetail .tit{
  text-align: center;
  font-size: 32rpx;
  font-weight: bold;
  padding-bottom: 30rpx;
  padding-top: 10rpx;
}
.atdetail .tit text{
  position: relative;
  z-index: 2;
}
.atdetail .tit text::before{
  content: '';
  position: absolute;
  border-bottom: 8rpx solid #3994FF;
  bottom: 2rpx;
  left: 0;
  right: 0;
  z-index: -1;
}
.atdetail .person{
  width: 200rpx;
  height: 200rpx;
  margin-top: 20rpx;
}
/* 底部css */
.foot{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  text-align: center;
  padding: 30rpx 0;
  font-size: 36rpx;
  background-color: #3994FF;
  font-size: 36rpx;
  color: #fff;
}
.step{
  background-color: #3994ff;
}
.step1{
  background-color: #999;
}
.step2{
  background-color: #3994FF;
}
.step3{
  background-color: #ccc;
}


.news_detail .content {
  padding: 30rpx 0;
}
rich-text {
  word-break: break-all;
}
.end {
  font-size: 32rpx;
  color: #999999;
  text-align: center;
  margin-top: 30rpx;
}
/* 推荐店铺 */
.store .content_store_title {
  font-size: 28rpx;
  font-weight: bold;
  margin-top: 20rpx;
}
.store .store_item {
  display: flex;
  border-radius: 5px;
  box-shadow: 0 0 3px rgba(0,0,0,0.1);
  padding: 20rpx;
  box-sizing: border-box;
  position: relative;
  margin-top: 20rpx;
  margin-bottom: 30rpx;
}
.store .store_item .cover {
  width: 140rpx;
  height: 140rpx;
  border-radius: 5px;
  margin-right: 20rpx;
}
.store .store_item .store_info {
  height: 140rpx;
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 10rpx 0;
  box-sizing: border-box;
}
.store .store_item .store_info .store_name {
  font-size: 28rpx;
  font-weight: bold;
}
.store .store_item .store_info .other_info text {
  font-size: 24rpx;
  color: #999999;
  margin-right: 20rpx;
}
.store .collection {
  position: absolute;
  right: 20rpx;
  top: 0;
  bottom: 0;
  display: flex;
  align-items: center;
}
.recommend,.comments {
  margin-top: 20rpx;
}
/* 相关推荐 */
.store_show {
  position: fixed;
  bottom: 160rpx;
  right: 20rpx;
  background-color: #0079FE;
  color: #ffffff;
  border-radius: 50px;
  padding: 10rpx 20rpx;
  font-size: 28rpx;
}
.store_title {
  display: flex;
  justify-content: center;
  align-items: center;
  height:90rpx;
  font-size: 32rpx;
  font-weight: 400;
  border-bottom: 1rpx solid #eeeeee;
  position: relative;
}
.store_title .img {
  position: absolute;
  right: 20rpx;
}
.store_list {
  padding-top: 10rpx;
  padding-bottom: 30rpx;
  box-sizing: border-box;
}
.store_list .store_item {
  display: flex;
  padding: 20rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
}
.store_list .store_item .cover {
  width: 170rpx;
  height: 170rpx;
  border-radius: 5px;
  margin-right: 20rpx;
}
.store_list .store_item .store_info {
  height: 180rpx;
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 5rpx 0;
  box-sizing: border-box;
}
.store_list .store_item .store_info .store_name {
  font-size: 28rpx;
  font-weight: bold;
  display: block;
  margin-bottom: 10rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.store_list .store_item .store_info .sentiment {
  font-size: 24rpx;
  color: #999999;
}
.store_list .store_item .store_info .sentiment text {
  margin-right: 20rpx;
}
.store_list .store_item .store_info .price {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  color: #999999;
  font-size: 28rpx;
}
.store_list .store_item .store_info .price .price_num {
  color: #FF7800;
  font-weight: bold;
  font-size: 36rpx;
}
.store_list .store_item .store_info .price .price_num .since {
  font-weight: normal;
  font-size: 28rpx;
}
.store_list .store_item .store_info .price button {
  margin: 0;
  width: 140rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: #0079FE;
  color: #ffffff;
  font-size: 28rpx;
  border-radius: 50px;
}
.store_close {
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #0079FE;
  font-size: 28rpx;
  box-shadow: 0 -3px 3px rgba(0,0,0,0.07);
