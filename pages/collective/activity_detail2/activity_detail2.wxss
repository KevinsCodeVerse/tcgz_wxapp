}
.inlist{
  padding: 46rpx 0;
}
.mytable{
  padding: 30rpx 30rpx;
}
.mytable .tr{
  display: flex;
  align-items: center;
  color: #666;
  font-size: 24rpx;
  padding: 10rpx 0;
}
.mytable .td:nth-child(1){
  
  text-align: left;
  width: 35%;
  display: flex;
  align-items: center;
}
.mytable .td:nth-child(2){
  text-align: center;
  width: 40%;
  overflow:hidden;
  text-overflow: ellipsis;
  display:-webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
}
.mytable .td:nth-child(3){
  text-align: right;
  width: 25%;
}
.mytable .th{
  font-weight: bold;
  color: #333;
  font-size: 28rpx;
}
.mytable image{
  max-width: 48rpx;
  height: 48rpx;
  object-fit: cover;
  border-radius: 50%;
  margin-right: 14rpx;
}
