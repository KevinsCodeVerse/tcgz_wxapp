.btn_box {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  height: 180rpx;
  align-items: center;  
  background-color: #fff;
  padding: 0 20rpx;
  box-shadow: 0rpx -6rpx 4rpx #eee;
}

.btn_box .btn {
  flex: 1;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #3994ff;
  border-radius: 16rpx;
  display: flex;
  color: #ffffff;
  justify-items: center;
  justify-content: center;
}

.list_box {
  padding: 20rpx 26rpx;
  padding-bottom: 250rpx;
}

.list_box .item_box { 
  background-color: #ffffff;
  border-radius: 12rpx;
  padding: 20rpx;
  margin-bottom: 22rpx;
}

.list_box .item_box .content_box {
  margin-bottom: 14rpx;
  display: flex;
}

.list_box .item_box .content_box .label {
  min-width: 150rpx;
  color: #777;
}



