  bottom: 1%;
  right: -4%;
  width: 50rpx;
  height: 48rpx;
  border-radius: 10rpx;
  border: none;
}
.check-box .item {
  position: relative;
}
.check-box checkbox {
  display: contents;
}
/* 选中后的背景样式 */
.check-box checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border: none;
  background: #4987f2;
}
/* 选中后的勾子样式 */
